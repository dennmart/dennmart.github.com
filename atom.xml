---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Dennis Martinez - Development Blog</title>
  <link href="http://development.dennmart.com/atom.xml" rel="self"/>
  <link href="http://development.dennmart.com/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://development.dennmart.com/</id>
  <author>
    <name>Dennis Martinez</name>
    <email>dennis@dennmart.com</email>
  </author>
  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://development.dennmart.com{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://development.dennmart.com{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>